<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="10dp"
        android:paddingTop="5dp"
        android:paddingEnd="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_code"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:textColor="#131314"/>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_dev_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp"
            android:text="âœ…"
            android:textColor="#232323"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@id/tv_dev_url"
            app:layout_constraintBottom_toBottomOf="@id/tv_dev_url"
            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_dev_url"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:maxLines="50"
            android:textColor="#232323"
            android:textSize="16sp"
            app:layout_constraintLeft_toRightOf="@+id/tv_dev_icon"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_code"
            tools:text="https://test.com/api/test" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_dev_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#646464"
            android:textSize="12sp"
            app:layout_constraintLeft_toLeftOf="@id/tv_dev_url"
            app:layout_constraintTop_toBottomOf="@+id/tv_dev_url"
            tools:text="20:00:00" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:background="#f7f7f7"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_dev_time" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>